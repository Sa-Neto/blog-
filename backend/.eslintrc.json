{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "arrow-body-style":"off",
      "no-undef":"off",
      "consistent-return":"off",
      "import/first": "off",
      "no-underscore-dangle": "off",
      "prefer-const": "off"
    }
}
